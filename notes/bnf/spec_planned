<program> ::= <global>*

<global> ::= <function> | <assignment>

<function> ::= "begin" <identifier> <parameter>* <statement>* "end"

<assignment> ::= <identifier> ":" <expression>

<statement> ::= 
	<function> | 
	<declaration> | 
	<assignment> | 
	<branch> | 
	<call> |
	<conditional-statement> | 
	<print> | 
	<exit>

<declaration> ::= <size> <identifier> | <size> <assignment>

<branch> ::= "goto" 

<call> ::= "call" <identifier>*

<conditional-statement> ::= "if" "(" <condition> ")" <statement>

<print> ::= "print" <number>

<exit> ::= "exit" <expression>


<parameters> ::= <size> <identifier>

<expression> ::= <number> | <binary-expression> | <group>

<group> ::= "(" <expression> ")"

<unary> ::= "!" <expression>

<binary> ::= <expression> <binary-operator> <expression>

<binary-operator> ::= "+" | "-" | "*" | "=" | "<" | ">"

<number> ::= <digit>*

<size> ::= w8 | w16 | w32 | w64 | f8 | f16 | f32 | f64

<digit> ::= "0" | "1" | "2" | ...
 
<identifier> ::= <letter> <character>*

<character> ::= <letter> | <digit>

<letter> ::= "a" | "b" | "c" | ... | "A" | "B" | "C" | ...






























